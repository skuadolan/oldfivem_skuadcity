{
  "name": "ghmattimysql",
  "version": "1.3.1",
  "description": "mysql middleware for fivem",
  "private": true,
  "scripts": {
    "install:core": "cd packages/ghmattimysql && npm i",
    "install:ui": "cd packages/ghmattimysql-ui && npm i",
    "postinstall": "run-p install:*",
    "build:core": "cd packages/ghmattimysql && npm run build",
    "build:ui": "cd packages/ghmattimysql-ui && npm run build",
    "build": "run-p build:*"
  },
  "author": "Matthias Mandelartz",
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
